<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Microsoft.Practices.Unity.Configuration"/>
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net, Version=1.2.10.0, Culture=neutral, PublicKeyToken=692fbea5521e1304"/>
  </configSections>
  <appSettings>
    <add key="LoginUser" value=""/>
    <add key="LoginPassword" value=""/>
    <add key="BackupUser" value=""/>
    <add key="BackupPassword" value=""/>
    <add key="DatabaseHost" value="localhost"/>
    <add key="DatabaseName" value="puntoventa"/>
    <add key="MySQLDumpOptions" value="--complete-insert --no-create-info --default-character-set=utf8"/>
    <add key="TargetFtpSource" value="ftp://127.0.0.1/luqueyvale"/>
    <add key="TargetFtpSecureUser" value=""/>
    <add key="TargetFtpSecurePass" value=""/>
    <add key="ApplicationThumbprint" value="E7C81DBE787480356FCD8E4E8F829B0F4DF57ECD"/>
  </appSettings>
  <connectionStrings>
    <add name="LeandroContext" connectionString="" providerName="MySql.Data.MySqlClient"/>
  </connectionStrings>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlConnectionFactory, EntityFramework" />
    <providers>
      <provider invariantName="MySql.Data.MySqlClient" type="MySql.Data.MySqlClient.MySqlProviderServices, 
        MySql.Data.Entity.EF6" />
    </providers>
  </entityFramework>
  <log4net>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="errorLogFile.txt"/>
      <appendToFile value="true"/>
      <rollingStyle value="Composite"/>
      <datePattern value="dd-MM-yyyy"/>
      <maxSizeRollBackups value="-1"/>
      <maximumFileSize value="1MB"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level %logger - %message%newline"/>
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="INFO"/>
        <levelMax value="ERROR"/>
      </filter>
    </appender>
    <root>
      <level value="ALL"/>
      <appender-ref ref="RollingFileAppender"/>
    </root>
  </log4net>
  <unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
    <sectionExtension type="LeandroSoftware.PuntoVenta.Datos.ContextoPrincipal.AppSettingParameterValueExtension, Datos"/>
    <container>
      <register type="LeandroSoftware.PuntoVenta.Datos.ContextoPrincipal.IDbContext, Datos" mapTo="LeandroSoftware.PuntoVenta.Datos.ContextoPrincipal.LeandroContext, Datos">
        <constructor>
          <param name="host" type="System.String">
            <appSettings appSettingsKey="DatabaseHost"/>
          </param>
          <param name="database" type="System.String">
            <appSettings appSettingsKey="DatabaseName"/>
          </param>
          <param name="user" type="System.String">
            <appSettings appSettingsKey="LoginUser"/>
          </param>
          <param name="pass" type="System.String">
            <appSettings appSettingsKey="LoginPassword"/>
          </param>
          <param name="key" type="System.String">
            <appSettings appSettingsKey="ApplicationThumbprint"/>
          </param>
        </constructor>
      </register>
      <register type="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.IBancaService, Servicios" mapTo="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.BancaService, Servicios"/>
      <register type="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.ICompraService, Servicios" mapTo="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.CompraService, Servicios"/>
      <register type="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.IContabilidadService, Servicios" mapTo="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.ContabilidadService, Servicios"/>
      <register type="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.ICuentaPorCobrarService, Servicios" mapTo="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.CuentaPorCobrarService, Servicios"/>
      <register type="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.ICuentaPorPagarService, Servicios" mapTo="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.CuentaPorPagarService, Servicios"/>
      <register type="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.IEgresoService, Servicios" mapTo="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.EgresoService, Servicios"/>
      <register type="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.IFacturacionService, Servicios" mapTo="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.FacturacionService, Servicios"/>
      <register type="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.IIngresoService, Servicios" mapTo="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.IngresoService, Servicios"/>
      <register type="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.ITrasladoService, Servicios" mapTo="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.TrasladoService, Servicios"/>
      <register type="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.IMantenimientoService, Servicios" mapTo="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.MantenimientoService, Servicios"/>
      <register type="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.IReporteService, Servicios" mapTo="LeandroSoftware.PuntoVenta.Servicios.ContextoPrincipal.ReporteService, Servicios"/>
    </container>
  </unity>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
